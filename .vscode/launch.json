    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/s3proxy/cmd",
            "args": ["--no-tls", "--level=-4", "--no-tagging", "--cache=memory"],
            "env": {
                "AWS_ACCESS_KEY_ID": "YOUR_KEY_ID",
                "AWS_SECRET_ACCESS_KEY": "YOUR_SECRET_ACCESS_KEY",
                "S3PROXY_HOST": "s3.us-west-XXX.backblazeb2.com",
                "S3PROXY_ENCRYPT_KEY": "some-totally-secret-and-random-key-to-encrypt-your-upload-clientside"
            }
        }
    ]
}